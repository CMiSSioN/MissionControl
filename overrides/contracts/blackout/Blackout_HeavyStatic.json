        }
      ]
    },
    {
      "dialogue": {
        "EncounterObjectGuid": "e0ca3227-ffbf-4088-a261-3d4e9ab7d4c5"
      },
      "name": "Dialogue_First_Point_Of_Interest",
      "overrideDialogueBucketId": "",
      "dialogueContent": [
        {
          "words": "Strange. There looks like there's been fighting here but it was a least a few weeks ago. Let's look up on ahead. Maybe we'll find something at the other base.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_DariusDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "e7e9f35b-7ed8-404e-9dae-69be61de2dd3",
          "cameraDistance": "Close",
          "cameraHeight": "Default",
          "revealRadius": -1
        }
      ]
    },
    {
      "dialogue": {
        "EncounterObjectGuid": "f404adfd-7613-4fc3-ae55-f025ac6d8213"
      },
      "name": "Dialogue_First_Point_Of_Interest_Trap",
      "overrideDialogueBucketId": "",
      "dialogueContent": [
        {
          "words": "What's this?! The turrets are activating. Incoming fire, Commander!",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_DariusDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "e7e9f35b-7ed8-404e-9dae-69be61de2dd3",
          "cameraDistance": "Medium",
          "cameraHeight": "Default",
          "revealRadius": -1
        }
      ]
    },
    {
      "dialogue": {
        "EncounterObjectGuid": "47c75ee8-8af6-46a8-98a3-418f754616c5"
      },
      "name": "Dialogue_First_Point_Of_Interest_Ambush",
      "overrideDialogueBucketId": "",
      "dialogueContent": [
        {
          "words": "We were told to expect interference. Time to shut you out.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_SE06_KarosasCaptainDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "e7e9f35b-7ed8-404e-9dae-69be61de2dd3",
          "cameraDistance": "Far",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "This isn't good, Commander. You're surrounded!",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_DariusDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "e7e9f35b-7ed8-404e-9dae-69be61de2dd3",
          "cameraDistance": "Far",
          "cameraHeight": "Default",
          "revealRadius": -1
        }
      ]
    },
    {
      "dialogue": {
        "EncounterObjectGuid": "399d54e8-522a-4576-b01f-14d705dc9d6b"
      },
      "name": "Dialogue_Post_2b",
      "overrideDialogueBucketId": "",
      "dialogueContent": [
        {
          "words": "Is everything ok down there, Commander? Looks like those turrets were hacked. Something is definitely not right here.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_DariusDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "e7e9f35b-7ed8-404e-9dae-69be61de2dd3",
          "cameraDistance": "Medium",
          "cameraHeight": "High",
          "revealRadius": -1
        }
      ]
    },
    {
      "dialogue": {
        "EncounterObjectGuid": "4ef16b5e-1486-4399-8835-b81026b22cac"
      },
      "name": "Dialogue_Second_Point_Of_Interest_3a",
      "overrideDialogueBucketId": "",
      "dialogueContent": [
        {
          "words": "Unidentified BattleMech lance. Declare yourself. You are violating a {TEAM_EMP.FactionDef.ShortName} zone",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_SE06_KarosasCaptainDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "21a03616-c88b-4edd-a9a9-b4dd54b46d6c",
          "cameraDistance": "Close",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "{TEAM_EMP.FactionDef.Demonym} radio operator. We are {COMPANY.CompanyName} and have been hired to check the sit rep here. You've been out of contact for weeks. Everything ok here?",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_DariusDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "21a03616-c88b-4edd-a9a9-b4dd54b46d6c",
          "cameraDistance": "Close",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "Everythings fi..<b>***STATIC***</b>",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_SE06_KarosasCaptainDefault",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "21a03616-c88b-4edd-a9a9-b4dd54b46d6c",
          "cameraDistance": "Close",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "Commander! It's a trap! Watch ou...<b>***STATIC***</b>",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_TeamLeader_Employer",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "21a03616-c88b-4edd-a9a9-b4dd54b46d6c",
          "cameraDistance": "Medium",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "Damn it! Gun them down now!",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_TeamLeader_Hostile",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "21a03616-c88b-4edd-a9a9-b4dd54b46d6c",
          "cameraDistance": "Far",
          "cameraHeight": "Default",
          "revealRadius": -1
        }
      ]
    },
    {
      "dialogue": {
        "EncounterObjectGuid": "d530f8a5-ce14-4af8-8da3-2020dab6def1"
      },
      "name": "Dialogue_Second_Point_Of_Interest_3b",
      "overrideDialogueBucketId": "",
      "dialogueContent": [
        {
          "words": "Commander! Are we glad to see you! {TEAM_EMP.FactionDef.Demonym} forward base {TGT_SYSTEM.name}-Alpha.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_TeamLeader_Employer",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "21a03616-c88b-4edd-a9a9-b4dd54b46d6c",
          "cameraDistance": "Close",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "We've been holding off assaults on our position for the past few weeks. We've fallen back to this base but we can't hold out for much longer.",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_TeamLeader_Employer",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "21a03616-c88b-4edd-a9a9-b4dd54b46d6c",
          "cameraDistance": "Medium",
          "cameraHeight": "Default",
          "revealRadius": -1
        },
        {
          "words": "We've got contacts! Incoming!",
          "wordsColor": {
            "r": 1,
            "g": 1,
            "b": 1,
            "a": 1
          },
          "selectedCastDefId": "castDef_SE01_CommVoice01",
          "emote": "Default",
          "audioName": "NONE",
          "cameraFocusGuid": "21a03616-c88b-4edd-a9a9-b4dd54b46d6c",
          "cameraDistance": "Far",
          "cameraHeight": "High",
          "revealRadius": -1
        }
      ]
    }
